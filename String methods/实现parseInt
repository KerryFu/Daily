// parseInt() 函数可解析一个字符串，并返回一个整数

function str2num(str) {
  var strArr = str.split('')
  var strArrNum = strArr.map(function (str) {
    return +str
  })
  console.log(strArrNum);
  var num = strArrNum.reduce(function (x, y) {
    return x * 10 + y
  })
  return num
}

str2num('123')    //123


